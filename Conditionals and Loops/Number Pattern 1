import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner s= new Scanner(System.in);
		int n = s.nextInt();
		int nst=1;
		int row=1;
		int value=1;
		while(row<=n) {
			value=row;
			
		for(int cst=1; cst <=row; cst++) {
			System.out.print(value);
			value++;
		}
		row++;
		nst++;
		System.out.println();
		}
	}
}	
